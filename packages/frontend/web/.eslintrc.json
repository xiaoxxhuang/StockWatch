{
    "env": {
      "browser": true,
      "es2021": true,
      "jest/globals": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": ["@typescript-eslint", "jest", "react-hooks", "import"],
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "react-hooks/exhaustive-deps": "error",
      "import/no-webpack-loader-syntax": "error"
    }
}